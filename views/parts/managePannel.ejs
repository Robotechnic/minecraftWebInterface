<section class="pageContener">
	<nav class="widgetSelectors">
		<a class="widgetSelectors__item" href="#status">Status</a>
		<a class="widgetSelectors__item" href="#console">Console</a>
		<a class="widgetSelectors__item" href="#configuration">Configuration</a>
		<a class="widgetSelectors__item" href="#joueurs">Joueurs</a>
	</nav>
	<section class="pageSection basePage visible center" id="status">
		<div class="statusDisplay">
			<h2><%=serverIp%></h2>
			<h3 class="status">Offline</h3>
			<nav class="serverActions">
				<button class="serverActions__button restart">
					<img src="images/restart.png">
				</button>
				<button class="serverActions__button start">
					<img src="images/powerButton.png">
				</button>
			</nav>
		</div>
		&nbsp;
	</section>
	<section class="pageSection" id="console">
		<div class="consoleView">
			<div class="consoleView__text consoleOutput" lines="all">
				&nbsp;
			</div>
			<form class="consoleView__input">
				<input type="text" name="commande" id="commande" class="consoleView__input__text">
			</form>
		</div>
	</section>
	<section class="pageSection" id="configuration">
		
	</section>
